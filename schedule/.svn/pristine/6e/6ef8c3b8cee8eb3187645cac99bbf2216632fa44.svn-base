/**
 * 收入统计
 */
public class StatTotalEntity {
	
	private String statTotalId;
	
	//-------------  line_class_info -------
	/**
	 * 时间
	 */
	private String orderDate;
	
	/**
	 * 班次
	 */
	private String orderStartTime;
	
	/**
	 * 班次ID
	 */
	private String lineClassId;
	
	
	//----------- line_base_info -----------
	/**
	 * 线路类型
	 * 0:上下班  1:旅游  (自由行)
	 */
	private Integer lineSubType;
	
	/**
	 * 线路名称
	 */
	private String lineName;
	
	/**
	 * 线路ID
	 */
	private String lineBaseId;
	
	/**
	 * 线路原价格
	 */
	private BigDecimal linePrice;
	
	/**
	 * 折扣率
	 */
	private String discountRate;
	
	
	//--------------- passenger_info ---------
	/**
	 * 乘客回显ID
	 */
	private String displayId;
	
	/**
	 * 乘客昵称
	 */
	private String nickName;
	

	/**
	 * 乘客ID
	 */
	private String passengerId;
	
	
	//-------------- mgr_business ------------
	/**
	 * 商家  商户简称
	 */
	private String brefName;
	
	/**
	 * 商户ID
	 */
	private String businessId;
	
	
	//-------------- driver_info -------------
	/**
	 * 司机  司机姓名
	 */
	private String driverName;
	
	/**
	 * 司机ID
	 */
	private String driverId;
	
	
	//----------------  stat_passenger_consum_detail --------
	
	/**
	 * 消费额度(收款金额)
	 */
	private BigDecimal consumeLimit;
	
	
	//----------------- lease_base ------------
	/**
	 * 购买类型(方式)
	 * 0:按次购买        1:包月购买
	 */
	private Integer buyType;
	
	
	//----------------  vehicle_info --------
	/**
	 * 车辆(车牌号)
	 */
	private String vehicleNumber;
	
	/**
	 * 车辆类型 0:大巴 1:中巴 2:小巴
	 */
	private String vehicleType;
	
	/**
	 * 品牌
	 */
	private String vehicleBrand;
	
	/**
	 * 车辆ID
	 */
	private String vehicleId;
	
	/**
	 * 订单号
	 * @return
	 */
	private String leaseOrderNo;
	
	/**
	 * 下单时间
	 * @return
	 */
	private String operatTime;
	


	public String getStatTotalId() {
		return statTotalId;
	}


	public void setStatTotalId(String statTotalId) {
		this.statTotalId = statTotalId;
	}


	public String getOrderDate() {
		return orderDate;
	}


	public void setOrderDate(String orderDate) {
		this.orderDate = orderDate;
	}


	public String getOrderStartTime() {
		return orderStartTime;
	}


	public void setOrderStartTime(String orderStartTime) {
		this.orderStartTime = orderStartTime;
	}


	public Integer getLineSubType() {
		return lineSubType;
	}


	public void setLineSubType(Integer lineSubType) {
		this.lineSubType = lineSubType;
	}


	public String getLineName() {
		return lineName;
	}


	public void setLineName(String lineName) {
		this.lineName = lineName;
	}


	public String getDisplayId() {
		return displayId;
	}


	public void setDisplayId(String displayId) {
		this.displayId = displayId;
	}


	public String getNickName() {
		return nickName;
	}


	public void setNickName(String nickName) {
		this.nickName = nickName;
	}


	public String getBrefName() {
		return brefName;
	}


	public void setBrefName(String brefName) {
		this.brefName = brefName;
	}



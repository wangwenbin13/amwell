package com.amwell.vo;

/**
 * 乘客评论信息VO
 */

public class PassengerCommentsVo{
	
	private String commentTime;//评论时间
	//乘客ID及昵称,中间用空格连接
	private String displayId;
	private String nickName;
	private String lineName;//线路名称
	private String leaseOrderNo;//订单号
	private String commentContext;//评论内容
	private Short commentStatus;//评论状态（0.显示 1.屏蔽）
	private String commentId;//评论id
	private int starPoint;
	private String advinces; 
	
	public String getCommentTime() {
		return commentTime;
	}
	public void setCommentTime(String commentTime) {
		this.commentTime = commentTime;
	}
	public String getLineName() {
		return lineName;
	}
	public void setLineName(String lineName) {
		this.lineName = lineName;
	}
	public String getLeaseOrderNo() {
		return leaseOrderNo;
	}
	public void setLeaseOrderNo(String leaseOrderNo) {
		this.leaseOrderNo = leaseOrderNo;
	}
	public String getCommentContext() {
		return commentContext;
	}
	public void setCommentContext(String commentContext) {
		this.commentContext = commentContext;
	}
	public String getNickName() {
		return nickName;
	}
	public void setNickName(String nickName) {
		this.nickName = nickName;
	}
	public String getDisplayId() {
		return displayId;
	}
	public void setDisplayId(String displayId) {
		this.displayId = displayId;
	}
	public Short getCommentStatus() {
		return commentStatus;
	}
	public void setCommentStatus(Short commentStatus) {
		this.commentStatus = commentStatus;
	}
	public String getCommentId() {
		return commentId;
	}
	public void setCommentId(String commentId) {
		this.commentId = commentId;
	}
	public int getStarPoint() {
		return starPoint;
	}
	public void setStarPoint(int starPoint) {
		this.starPoint = starPoint;
	}
	public String getAdvinces() {
		return advinces;
	}
	public void setAdvinces(String advinces) {
		this.advinces = advinces;
	}
	
}
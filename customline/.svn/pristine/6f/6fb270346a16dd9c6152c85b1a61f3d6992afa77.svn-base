package com.amwell.vo.bc;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 * 
 * @author wangwenbin
 *
 * 2014-12-2
 */
/**
 * 包车退票实体类
 */
public class BcReturnHistoryEntity implements Serializable{

	/**
	 * 包车退票ID
	 */
	private String bcReturnId;
	
	/**
	 * 包车订单ID
	 */
	private String orderId;
	
	/**
	 * 包车订单号
	 */
	private String orderNo;
	
	/**
	 * 包车线路ID
	 * @return
	 */
	private String bc_line_id;
	
	/**
	 * 乘客ID
	 * @return
	 */
	private String passengerId;
	
	/**
	 * 商户ID
	 * @return
	 */
	private String businessId;
	
	/**
	 * 原订单金额
	 */
	private BigDecimal oldMoney;
	
	/**
	 * 实际退款金额
	 */
	private BigDecimal bcReturnMoney;
	
	/**
	 * 实际退款费率
	 */
	private String realPer;
	
	/**
	 * 操作人ID
	 * @return
	 */
	private String operatorId;
	
	/**
	 * 操作时间
	 * @return
	 */
	private String operateTime;

	public String getBcReturnId() {
		return bcReturnId;
	}

	public void setBcReturnId(String bcReturnId) {
		this.bcReturnId = bcReturnId;
	}

	public String getOrderId() {
		return orderId;
	}

	public void setOrderId(String orderId) {
		this.orderId = orderId;
	}

	public String getBc_line_id() {
		return bc_line_id;
	}

	public void setBc_line_id(String bcLineId) {
		bc_line_id = bcLineId;
	}

	public String getPassengerId() {
		return passengerId;
	}

	public void setPassengerId(String passengerId) {
		this.passengerId = passengerId;
	}

	public String getBusinessId() {
		return businessId;
	}

	public void setBusinessId(String businessId) {
		this.businessId = businessId;
	}

	public BigDecimal getBcReturnMoney() {
		return bcReturnMoney;
	}

	public void setBcReturnMoney(BigDecimal bcReturnMoney) {
		this.bcReturnMoney = bcReturnMoney;
	}

	public String getRealPer() {
		return realPer;
	}

	public void setRealPer(String realPer) {
		this.realPer = realPer;
	}

	public String getOperatorId() {
		return operatorId;
	}

	public void setOperatorId(String operatorId) {
		this.operatorId = operatorId;
	}

	public String getOperateTime() {
		return operateTime;
	}

	public void setOperateTime(String operateTime) {
		this.operateTime = operateTime;
	}

	public BigDecimal getOldMoney() {
		return oldMoney;
	}

	public void setOldMoney(BigDecimal oldMoney) {
		this.oldMoney = oldMoney;
	}

	public String getOrderNo() {
		return orderNo;
	}

	public void setOrderNo(String orderNo) {
		this.orderNo = orderNo;
	}
	
	
}

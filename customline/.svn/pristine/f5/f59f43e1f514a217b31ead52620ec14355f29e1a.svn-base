package com.amwell.vo;

import java.math.BigDecimal;

import com.amwell.entity.base.BaseEntity;

public class BalanceSetVo extends BaseEntity{

	private static final long serialVersionUID = 5413018659232231104L;

	private String balance_set_id;

	private String setName;// 模板名称
	
	private Integer setType;//模板类型,1：保底 2：保底+分成 3：分成

	private Integer minimumNum;//保底人数

	private BigDecimal platformPercent;//平台所占分成百分比

	private BigDecimal merchantPercent;//承运商所占百分比

	private String createBy;

	private String createOn;
	
	private Integer count;

	public String getBalance_set_id() {
		return balance_set_id;
	}

	public void setBalance_set_id(String balance_set_id) {
		this.balance_set_id = balance_set_id;
	}

	public String getSetName() {
		return setName;
	}

	public void setSetName(String setName) {
		this.setName = setName;
	}

	public Integer getMinimumNum() {
		return minimumNum;
	}

	public void setMinimumNum(Integer minimumNum) {
		this.minimumNum = minimumNum;
	}

	public BigDecimal getPlatformPercent() {
		return platformPercent;
	}

	public void setPlatformPercent(BigDecimal platformPercent) {
		this.platformPercent = platformPercent;
	}

	public BigDecimal getMerchantPercent() {
		return merchantPercent;
	}

	public void setMerchantPercent(BigDecimal merchantPercent) {
		this.merchantPercent = merchantPercent;
	}

	public String getCreateBy() {
		return createBy;
	}

	public void setCreateBy(String createBy) {
		this.createBy = createBy;
	}

	public String getCreateOn() {
		return createOn;
	}

	public void setCreateOn(String createOn) {
		this.createOn = createOn;
	}

	public Integer getCount() {
		return count;
	}

	public void setCount(Integer count) {
		this.count = count;
	}

	public Integer getSetType() {
		return setType;
	}

	public void setSetType(Integer setType) {
		this.setType = setType;
	}


	
	
}

package com.amwell.vo;

/**
 *帮助回馈信息
 *
 */
public class HelpFeedbackVo {
	
	

	private String feedbackId;//帮助回馈信息ID
	
	private String passengerId;//乘客ID
	
	private String feedbackTime;//帮助回馈时间
	
	private String feedbackContext;//帮助回馈内容
	
	private String phoneNo;//用户联系方式
	
	private String nickName;//乘客名称 
	
	private String telephone;//乘客电话
	
	private String displayId;//乘客ID
	
	private String handleFeedbackingRemark; //处理反馈中备注
	
	private String handleFeedbackendRemark; //处理反馈最终备注
	
	private String middleHandleUser;//中间处理人
	private String middleHandleTime;//中间处理时间
	
	private String handleUser; // 处理人(只记录最后处理人)
	
	private String handleTime;//操作时间
	
	private int status; // 反馈处理状态(1未处理,2处理中,3已处理)
	
	public String getNickName() {
		return nickName;
	}

	public void setNickName(String nickName) {
		this.nickName = nickName;
	}

	public String getTelephone() {
		return telephone;
	}

	public void setTelephone(String telephone) {
		this.telephone = telephone;
	}
	
	public String getMiddleHandleTime() {
		return middleHandleTime;
	}

	public void setMiddleHandleTime(String middleHandleTime) {
		this.middleHandleTime = middleHandleTime;
	}

	public String getFeedbackId() {
		return feedbackId;
	}

	public void setFeedbackId(String feedbackId) {
		this.feedbackId = feedbackId;
	}

	public String getPassengerId() {
		return passengerId;
	}

	public void setPassengerId(String passengerId) {
		this.passengerId = passengerId;
	}

	public String getFeedbackTime() {
		return feedbackTime;
	}

	public void setFeedbackTime(String feedbackTime) {
		this.feedbackTime = feedbackTime;
	}

	public String getFeedbackContext() {
		return feedbackContext;
	}

	public void setFeedbackContext(String feedbackContext) {
		this.feedbackContext = null!=feedbackContext?feedbackContext.trim().replaceAll("[\\t\\n\\r]", ""):feedbackContext;
	}

	public String getDisplayId() {
		return displayId;
	}

	public void setDisplayId(String displayId) {
		this.displayId = displayId;
	}

	

	public String getHandleFeedbackingRemark() {
		return handleFeedbackingRemark;
	}

	public void setHandleFeedbackingRemark(String handleFeedbackingRemark) {
		this.handleFeedbackingRemark = handleFeedbackingRemark;
	}

	public String getHandleFeedbackendRemark() {
		return handleFeedbackendRemark;
	}

	public void setHandleFeedbackendRemark(String handleFeedbackendRemark) {
		this.handleFeedbackendRemark = null!=handleFeedbackendRemark?handleFeedbackendRemark.trim().replaceAll("[\\t\\n\\r]", ""):handleFeedbackendRemark;
	}

	public String getHandleUser() {
		return handleUser;
	}

	public void setHandleUser(String handleUser) {
		this.handleUser = handleUser;
	}

	public String getHandleTime() {
		return handleTime;
	}

	public void setHandleTime(String handleTime) {
		this.handleTime = handleTime;
	}

	public int getStatus() {
		return status;
	}

	public void setStatus(int status) {
		this.status = status;
	}

	public String getMiddleHandleUser() {
		return middleHandleUser;
	}

	public void setMiddleHandleUser(String middleHandleUser) {
		this.middleHandleUser = middleHandleUser;
	}

	public String getPhoneNo() {
		return phoneNo;
	}

	public void setPhoneNo(String phoneNo) {
		this.phoneNo = phoneNo;
	}
	

}

@charset "utf-8";
/* CSS validate */
.tipTable{background:#f7e7e9;border:1px solid #efb5c0;display:none;filter:alpha(opacity=80);overflow: hidden;position: absolute;z-index:5;empty-cells:hide;opacity:0.8;color:#d1261e;height:21px;font-size:12px;width:auto;line-height:20px;padding:0 5px;margin-left:100px;}
.onCorrect{background: url(../images/right_ico.png) no-repeat left center;margin-left:4px;display:inline-block;width:15px;height:11px;}/* CSS popPage */
.onCorrectNew{background: url(../images/right_ico.png) no-repeat left center;margin-left:4px;display:inline-block;width:15px;height:11px;}/* CSS popPage */
.hideBody{position: absolute;width:100%;height:1080px;z-index:10;display:none;background:#000;filter:alpha(opacity=50);-moz-opacity:0.5;opacity:0.5;z-index:100}
.mainBody{position: absolute;top:0;left:15px;width:99%;height:1080px;z-index:10;display:none;background:#000;filter:alpha(opacity=50);-moz-opacity:0.5;opacity:0.5;z-index:100}
.showPage{position: absolute;top:0;margin: 0 auto;left: 44%;margin-left: -500px;filter:alpha(opacity=100);-moz-opacity:1;opacity:1;z-index:100}
/* CSS Document */
body{
	background:#e1eff9;
	position:relative;
	}
.main,.footer{min-width:1360px;}
.index-nodate{
	display:inline-block;
	width:26px;
	height:26px;

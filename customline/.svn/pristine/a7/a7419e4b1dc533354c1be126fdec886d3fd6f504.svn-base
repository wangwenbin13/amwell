package com.amwell.vo.app;

/**
 * 订单_乘客订单表
 */

@SuppressWarnings("all")
public class LeaseBaseInfo implements java.io.Serializable {

	private String leaseOrderId;		//乘客订单ID
	private String leaseOrderNo;		//乘客订单号
	private String lineBaseId;			//线路信息ID
	private String slineId;             //子线路ID
	private String ridesInfo;			//购买信息(乘车次数)
	private String buyType;				//购买类型 0:按次购买 1:包月购买
	private String discountRate;		//优惠折扣率
	private String totalCount;			//费用总计
	private String leaseOrderTime;		//下单时间
	private String payStatus;			//支付状态 0:待支付 1:交易成功 2:已失效
	private String businessId;			//商户ID
	private String passengerId;			//乘客ID
	private String comment;				//评价次数
	private String temp;				//临时字段
	private String ustation;            //上车站点
	private String dstation;            //下车站点
	
	
	public String getTemp() {
		return temp;
	}
	public void setTemp(String temp) {
		this.temp = temp;
	}
	public String getPassengerId() {
		return passengerId;
	}
	public void setPassengerId(String passengerId) {
		this.passengerId = passengerId;
	}
	public String getComment() {
		return comment;
	}
	public void setComment(String comment) {
		this.comment = comment;
	}
	public String getLeaseOrderId() {
		return leaseOrderId;
	}
	public void setLeaseOrderId(String leaseOrderId) {
		this.leaseOrderId = leaseOrderId;
	}
	public String getLeaseOrderNo() {
		return leaseOrderNo;
	}
	public void setLeaseOrderNo(String leaseOrderNo) {
		this.leaseOrderNo = leaseOrderNo;
	}
	public String getLineBaseId() {
		return lineBaseId;
	}
	public void setLineBaseId(String lineBaseId) {
		this.lineBaseId = lineBaseId;
	}
	public String getRidesInfo() {
		return ridesInfo;
	}
	public void setRidesInfo(String ridesInfo) {
		this.ridesInfo = ridesInfo;
	}
	public String getBuyType() {
		return buyType;
	}
	public void setBuyType(String buyType) {
		this.buyType = buyType;
	}
	public String getDiscountRate() {
		return discountRate;
	}
	public void setDiscountRate(String discountRate) {
		this.discountRate = discountRate;
	}
	public String getTotalCount() {
		return totalCount;
	}
	public void setTotalCount(String totalCount) {
		this.totalCount = totalCount;
	}
	public String getLeaseOrderTime() {
		return leaseOrderTime;
	}
	public void setLeaseOrderTime(String leaseOrderTime) {
		this.leaseOrderTime = leaseOrderTime;
	}
	public String getPayStatus() {
		return payStatus;
	}
	public void setPayStatus(String payStatus) {
		this.payStatus = payStatus;
	}
	public String getBusinessId() {
		return businessId;
	}
	public void setBusinessId(String businessId) {
		this.businessId = businessId;
	}
	public String getSlineId() {
		return slineId;
	}
	public void setSlineId(String slineId) {
		this.slineId = slineId;
	}
	public String getUstation() {
		return ustation;
	}
	public void setUstation(String ustation) {
		this.ustation = ustation;
	}
	public String getDstation() {
		return dstation;
	}
	public void setDstation(String dstation) {
		this.dstation = dstation;
	}
	
}
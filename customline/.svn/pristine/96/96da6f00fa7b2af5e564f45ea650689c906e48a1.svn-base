package com.amwell.vo;

import java.math.BigDecimal;

import com.amwell.entity.base.BaseEntity;

public class LineAllowanceVo extends BaseEntity {

	private static final long serialVersionUID = -8746497446337155612L;

	private String allowanceId;

	private String lineBaseId;

	private BigDecimal ownAllowance;

	private BigDecimal otherAllowance;

	private Integer statusFlag;

	private String operateBy;

	private String operateOn;

	public String getAllowanceId() {
		return allowanceId;
	}

	public void setAllowanceId(String allowanceId) {
		this.allowanceId = allowanceId;
	}

	public String getLineBaseId() {
		return lineBaseId;
	}

	public void setLineBaseId(String lineBaseId) {
		this.lineBaseId = lineBaseId;
	}

	public BigDecimal getOwnAllowance() {
		return ownAllowance;
	}

	public void setOwnAllowance(BigDecimal ownAllowance) {
		this.ownAllowance = ownAllowance;
	}

	public BigDecimal getOtherAllowance() {
		return otherAllowance;
	}

	public void setOtherAllowance(BigDecimal otherAllowance) {
		this.otherAllowance = otherAllowance;
	}

	public Integer getStatusFlag() {
		return statusFlag;
	}

	public void setStatusFlag(Integer statusFlag) {
		this.statusFlag = statusFlag;
	}

	public String getOperateBy() {
		return operateBy;
	}

	public void setOperateBy(String operateBy) {
		this.operateBy = operateBy;
	}

	public String getOperateOn() {
		return operateOn;
	}

	public void setOperateOn(String operateOn) {
		this.operateOn = operateOn;
	}

}

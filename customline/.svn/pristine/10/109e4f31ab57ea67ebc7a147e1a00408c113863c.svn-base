package com.amwell.vo;

/**
 * 提现申请信息
 */

public class DriverWithdrawAskforVo implements java.io.Serializable {
	
	private static final long serialVersionUID = -5915639493055052407L;
	
	private String askforId;//提现申请id
	private String askforTime;//申请时间
	private String driverName;//司机姓名
	private String telephone;//手机号码
	private String businessName;//商家名称
	private String amount;//提现金额
	private String withdrawType;//提现方式（1.支付宝 2.微信 3.银联）
	private String withdrawAccount;//提现账户
	private String handleType;//申请处理状态（0.未处理 1.已处理）
	private String handleBy;//处理人姓名
	private String handleOn;//处理时间
	
	public String getAskforId() {
		return askforId;
	}
	public void setAskforId(String askforId) {
		this.askforId = askforId;
	}
	public String getAskforTime() {
		return askforTime;
	}
	public void setAskforTime(String askforTime) {
		this.askforTime = askforTime;
	}
	public String getDriverName() {
		return driverName;
	}
	public void setDriverName(String driverName) {
		this.driverName = driverName;
	}
	public String getTelephone() {
		return telephone;
	}
	public void setTelephone(String telephone) {
		this.telephone = telephone;
	}
	public String getBusinessName() {
		return businessName;
	}
	public void setBusinessName(String businessName) {
		this.businessName = businessName;
	}
	public String getAmount() {
		return amount;
	}
	public void setAmount(String amount) {
		this.amount = amount;
	}
	public String getWithdrawType() {
		return withdrawType;
	}
	public void setWithdrawType(String withdrawType) {
		this.withdrawType = withdrawType;
	}
	public String getWithdrawAccount() {
		return withdrawAccount;
	}
	public void setWithdrawAccount(String withdrawAccount) {
		this.withdrawAccount = withdrawAccount;
	}
	public String getHandleType() {
		return handleType;
	}
	public void setHandleType(String handleType) {
		this.handleType = handleType;
	}
	public String getHandleBy() {
		return handleBy;
	}
	public void setHandleBy(String handleBy) {
		this.handleBy = handleBy;
	}
	public String getHandleOn() {
		return handleOn;
	}
	public void setHandleOn(String handleOn) {
		this.handleOn = handleOn;
	}
}
<%@include file="../v1/template/footJavascript.jsp" %>
<script>
 	var openId = '${openId}', passenger = $('#passengerIpt').val();
 	openId && localStorage.setItem("openId", openId);
 	// 处理用户
 	if (passenger)
 		localStorage.setItem("MyUserInfo", passenger);
 	Zepto('.page-12-1 .t3,.page-12-1 .t4').tap(function () {
		var user = getLoginUser();
		if (!user)
			forwardHash('#login');
		else
 			location.href = '${contextPath}/games/anniversary/buyTicket?uid=' + user['a2'] + '&openId=' + openId;
	});
</script>

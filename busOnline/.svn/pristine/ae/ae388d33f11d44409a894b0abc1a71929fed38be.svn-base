package com.bean.games.fcz;

/**
 * 参与活动的盟主
 * 
 * @author shawn.zheng
 * 
 */
public class FczOwner {
	/**
	 * 主键
	 */
	private String storeId;
	/**
	 * 微信用户id
	 */
	private String openId;
	/**
	 * 微信用户昵称
	 */
	private String nickname;
	/**
	 * 手机号码
	 */
	private String telephone;
	/**
	 * 累计分数
	 */
	private int totalPoint;
	/**
	 * 用户兑换的奖品
	 */
	private String prideName;
	/**
	 * 用户可以玩的次数
	 */
	private int availablePlayNum;
	/**
	 * 是否已经分享
	 */
	private String isShare;

	public String getStoreId() {
		return storeId;
	}

	public void setStoreId(String storeId) {
		this.storeId = storeId;
	}

	public String getOpenId() {
		return openId;
	}

	public void setOpenId(String openId) {
		this.openId = openId;
	}

	public String getNickname() {
		return nickname;
	}

	public void setNickname(String nickname) {
		this.nickname = nickname;
	}

	public String getTelephone() {
		return telephone;
	}

	public void setTelephone(String telephone) {
		this.telephone = telephone;
	}

	public int getTotalPoint() {
		return totalPoint;
	}

	public void setTotalPoint(int totalPoint) {
		this.totalPoint = totalPoint;
	}

	public String getPrideName() {
		return prideName;
	}

	public void setPrideName(String prideName) {
		this.prideName = prideName;
	}

	public int getAvailablePlayNum() {
		return availablePlayNum;
	}

	public void setAvailablePlayNum(int availablePlayNum) {
		this.availablePlayNum = availablePlayNum;
	}

	public String getIsShare() {
		return isShare;
	}

	public void setIsShare(String isShare) {
		this.isShare = isShare;
	}

}
